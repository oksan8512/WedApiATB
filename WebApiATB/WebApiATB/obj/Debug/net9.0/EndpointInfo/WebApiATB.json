{
  "openapi": "3.0.4",
  "info": {
    "title": "WebApiATB",
    "version": "1.0"
  },
  "paths": {
    "/api/Account/Register": {
      "post": {
        "tags": [
          "Account"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "Email": {
                    "type": "string",
                    "description": "Пошта користувача",
                    "example": "bib@gmail.com"
                  },
                  "FirstName": {
                    "type": "string",
                    "description": "Ім'я користувача",
                    "example": "Василь"
                  },
                  "LastName": {
                    "type": "string",
                    "description": "Прізвище",
                    "example": "Дубко"
                  },
                  "Image": {
                    "type": "string",
                    "description": "Аватар користувача",
                    "format": "binary"
                  },
                  "Password": {
                    "type": "string",
                    "description": "Пароль",
                    "example": "123456"
                  }
                }
              },
              "encoding": {
                "Email": {
                  "style": "form"
                },
                "FirstName": {
                  "style": "form"
                },
                "LastName": {
                  "style": "form"
                },
                "Image": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Account/Login": {
      "post": {
        "tags": [
          "Account"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Account/GoogleLogin": {
      "get": {
        "tags": [
          "Account"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Account/GoogleCallback": {
      "get": {
        "tags": [
          "Account"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Categories": {
